import sys

if len(sys.argv) != 3:
    print("usage: <db> <readIdFile>")
    sys.exit(1)

strDb = sys.argv[1]

if not strDb.endswith(".db"):
    strDb = strDb + ".db"

if sys.argv[2] == "-":
    fin = sys.stdin
else:
    fin = open(sys.argv[2], "r")


all_file = fin.read().strip()  # Read and remove any extra new line
all_file_list = all_file.split('\n')  # make a list of lines
nIDlist = [[int(each_int) for each_int in line.split()] for line in all_file_list]  # make list of list and convert to int 

